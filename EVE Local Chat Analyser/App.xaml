<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:ui="clr-namespace:EveLocalChatAnalyser.Ui"
    xmlns:eve="clr-namespace:EveLocalChatAnalyser"
             xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="EveLocalChatAnalyser.App"
                xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:collections="clr-namespace:System.Collections;assembly=mscorlib"
             xmlns:model="clr-namespace:EveLocalChatAnalyser.Model"
             xmlns:wormholes="clr-namespace:EveLocalChatAnalyser.Ui.Wormholes"
             StartupUri="Ui\MainWindow.xaml" DispatcherUnhandledException="Application_DispatcherUnhandledException" ShutdownMode="OnMainWindowClose">
    <Application.Resources>
        <wormholes:EnumBooleanConverter x:Key="EnumBooleanConverter" />
        <ui:ColorConverter x:Key="ColorConverter"/>
        <ui:HistoryConverter x:Key="HistoryConverter" />
        <ui:VisibilityConverter x:Key="VisibilityConverter" />
        <BitmapImage x:Key="MapImage" UriSource="Resources\map.png" />
    </Application.Resources>
</Application>